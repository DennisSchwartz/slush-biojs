{
  "name": "<%= appNameSlug %>",
  "description": "<%= appDescription %>",
  "version": "<%= appVersion %>",
  "homepage": "https://github.com/<%= userName %>/<%= appNameSlug %>",
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/<%= userName %>/<%= appNameSlug %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= userName %>/<%= appNameSlug %>/issues"
  },
  "licenses": [
    {
      "type": "<%= license %>",
      "url": "https://github.com/<%= userName %>/<%= appNameSlug %>/blob/master/LICENSE"
    }
  ],
  "main": "lib/<%= appNameSlug %>",
  "scripts": { <% if (tests) { %>
    "test": "gulp", <% } %>
	"build": "gulp build",
    "build-browser": "gulp build-browser",
	"build-browser-min": "gulp build-browser-gzip",
	"prepublish": "gulp",
	"sniper": "biojs-sniper .",
    "watch": "gulp watch"
  },
  "dependencies": {},
  "devDependencies": {<% if (vis){ %>
	"biojs-sniper": "^0.0.4", <% } %> <% if (coverage){ %>
    "blanket": "^1.1.6", <% } %>
    "browserify": "5.x", <% if (tests){ %>
    "chai": "^1.9.1", <% } if (coverage){ %>
    "coveralls": "^2.11.1", <% } %>
	"del": "^0.1.3",
    "gulp": "^3.8.8",
	"gulp-chmod": "^1.1.1", <% if (coverage){ %>
    "gulp-coveralls": "^0.1.3", <% } %>
    "gulp-gzip": "0.0.8", <% if (jshint){ %>
    "gulp-jshint": "^1.8.4", <% } if(tests){ %>
    "gulp-mocha": "^1.0.0", <% if (vis && phantomjs){ %>
    "gulp-mocha-phantomjs": "^0.5.0", <% }} %>
    "gulp-rename": "^1.2.0",
	"gulp-streamify": "^0.0.5",
    "gulp-uglify": "^1.0.1",
	"gulp-util": "^3.0.1",
    "gulp-watch": "^1.0.2",
    "mkdirp": "^0.5.0", <% if (tests){ %>
    "mocha": "~1.20.1", <% if (coverage){ %>
    "mocha-lcov-reporter": "0.0.1", <% }} %>
	"vinyl-source-stream": "^1.0.0",
	"watchify": "^1.0.6"
  }, <% if (coverage){ %>
  "config": {
    "blanket": {
      "pattern": [
        "lib"
      ],
      "data-cover-never": "node_modules"
    },
    "travis-cov": {
      "threshold": 60
    }
  }, <% } %>
  "shields": {
    "test": {
      "img": "https://travis-ci.org/<%= userName %>/<%= appNameSlug %>.svg?branch=master",
      "href": "https://travis-ci.org/<%= userName %>/<%= appNameSlug %>"
    }
  }, <% if(vis){ %>
  "sniper": {
     "js": [ "/build/<%= appNameVar %>.js"],
     "snippets": ["snippets"],
     "first": "simple_example"
   }, <% } %>
  "keywords": <%= keywordList %>
}
